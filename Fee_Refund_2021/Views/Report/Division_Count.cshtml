@model IEnumerable<Fee_Refund_2021.Models.Division_Report_Model>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Division_Count</title>
    <link href="~/Design/layout/styles/Print.css" rel="stylesheet" />
    <style>
        .modal-backdrop {
            position: unset;
        }

        #Student_Report, th, td, tr {
            border: 2px solid;
            height: 40px;
            width: auto;
        }

        table {
            table-layout: fixed;
            page-break-after: auto
        }

        tr {
            page-break-inside: avoid;
            page-break-after: auto
        }

        td {
            page-break-inside: avoid;
            page-break-after: auto
        }

        #wrapper {
            width: 100%;
            height: 400px;
            border: 1px solid black;
        }

        button {
            height: 20px;
            position: relative;
            margin: -20px -50px;
            width: 100px;
            top: 50%;
            left: 50%;
        }



        @@media print {
            #btn_final_submit {
                display: none;
            }
        }
    </style>
</head>
<body>
    <div class="book">
        <div class="page">
            <div class="subpage">
                <div class="container">
                    
                    <div align="center">
                        <h5>
                            <span style="color:black;">माध्यमिक शालांत प्रमाणपत्र परीक्षा (इ. १० वी) व उच्च माध्यमिक प्रमाणपत्र परीक्षा (इ. १२ वी) साठी नोंदणीकृत विद्यार्थ्यांना परीक्षा शुल्क परतावा <br />Division Wise Count<br />@DateTime.Now.ToString("dd/MM/yyyy hh:mm:ss tt")</span>
                        </h5><br /><br />
                            <table class="borderTable" id="Student_Report">
                                <thead>
                                    <tr>
                                        <td>Division Code</td>
                                        <td>Division Name</td>
                                        <td>Total Register</td>
                                        <td>Complete</td>
                                        <td>InComplete</td>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item.Division_Code</td>
                                            <td>@item.Division_Name</td>
                                            <td>@item.Total_Register</td>
                                            <td>@item.Complete</td>
                                            <td>@item.InComplete</td>
                                        </tr>

                                    }
                                </tbody>
                            </table>

</div>
                    
                </div>

            </div>

        </div>

    </div>
</body>
</html>
